<?xml version="1.0" encoding="UTF-8" ?>
<install version="1.5" type="plugin" group="vmpayment" >
    <name>SveaWebPay library</name>
    <creationDate>January 2014</creationDate>
    <author>Svea Ekonomi AB WebPay</author>
    <authorUrl>http://www.sveawebpay.se</authorUrl>
    <copyright>Copyright (C) 2012 SveaWebPay - Svea Ekonomi</copyright>
    <license> http://www.apache.org/licenses/LICENSE-2.0</license>
    <version>2.4.10</version>
    <description>SveaWebPay library for SweaWebPay payment plugins for Virtuemart 2. Do not use as payment.</description>
    <files>
	<filename plugin="svealib">sveahelper.php</filename>
        <folder>integrationlib</folder>
        <folder>assets</folder>
    </files>
    <languages folder="assets">
       <!--all Svea payment methods use the translations from sveainvoice-->
	<language tag="en-GB">en-GB/en-GB.plg_vmpayment_sveainvoice.ini</language>
        <language tag="en-GB">en-GB/en-GB.plg_vmpayment_sveainvoice.sys.ini</language>
	<language tag="sv-SE">sv-SE/sv-SE.plg_vmpayment_sveainvoice.ini</language>
        <language tag="sv-SE">sv-SE/sv-SE.plg_vmpayment_sveainvoice.sys.ini</language>
	<language tag="de-DE">de-DE/de-DE.plg_vmpayment_sveainvoice.ini</language>
        <language tag="de-DE">de-DE/de-DE.plg_vmpayment_sveainvoice.sys.ini</language>
        <language tag="fi-FI">fi-FI/fi-FI.plg_vmpayment_sveainvoice.ini</language>
        <language tag="fi-FI">fi-FI/fi-FI.plg_vmpayment_sveainvoice.sys.ini</language>
        <language tag="nl-NL">nl-NL/nl-NL.plg_vmpayment_sveainvoice.ini</language>
        <language tag="nl-NL">nl-NL/nl-NL.plg_vmpayment_sveainvoice.sys.ini</language>
        <language tag="no-NO">no-NO/no-NO.plg_vmpayment_sveainvoice.ini</language>
        <language tag="no-NO">no-NO/no-NO.plg_vmpayment_sveainvoice.sys.ini</language>
        <language tag="da-DK">da-DK/da-DK.plg_vmpayment_sveainvoice.ini</language>
        <language tag="da-DK">da-DK/da-DK.plg_vmpayment_sveainvoice.sys.ini</language>
    </languages>
</install>
